package com.example.demo.dto;

public class CertificateType {

	   private Long id;

	    private String code;    // Example: REV-101
	    private String title;   // Example: Community Certificate
		public Long getId() {
			return id;
		}
		public void setId(Long id) {
			this.id = id;
		}
		public String getCode() {
			return code;
		}
		public void setCode(String code) {
			this.code = code;
		}
		public String getTitle() {
			return title;
		}
		public void setTitle(String title) {
			this.title = title;
		}
	    
	    
}
